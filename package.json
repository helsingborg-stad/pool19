{
  "name": "git19",
  "version": "0.0.1",
  "description": "Boilerplate repository for nodejs based LABS projects.",
  "main": "",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start-old": "NODE_ENV=production node src/server.js",
    "watch-server": "nodemon server/index.js",
    "start": "node server/index.js",
    "dev": "nodemon server/index.js | ./node_modules/.bin/pino-colada",
    "test": "NODE_ENV=test nyc ./node_modules/.bin/mocha src/components/**/*.test.js",
    "test:watch": "NODE_ENV=test nyc ./node_modules/.bin/mocha src/components/**/*.test.js --watch",
    "lint": "eslint src/*",
    "migrate:latest": "./node_modules/.bin/knex --cwd src/db/ migrate:latest",
    "migrate:rollback": "./node_modules/.bin/knex --cwd src/db/ migrate:rollback",
    "setup": "node setup.js",
    "heroku-postbuild": "cd client && yarn install --only=dev && yarn install && yarn build"
  },
  "author": "Helsingborg Stad",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-asserttype": "^1.0.5",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1",
    "pino-colada": "^1.4.4"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.1",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "bookshelf": "^0.15.1",
    "dotenv": "^8.0.0",
    "easy-soap-request": "^2.5.0",
    "express": "^4.16.4",
    "express-pino-logger": "^4.0.0",
    "json-api-serializer": "^2.1.0",
    "json-schema-ref-parser": "^7.1.1",
    "knex": "^0.17.1",
    "mysql": "^2.17.1",
    "pino": "^5.12.5",
    "redis": "^2.8.0",
    "soap": "^0.27.1",
    "swagger-ui-express": "^4.0.2",
    "ws": "^7.0.0",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "fs-extra": "^8.0.1",
    "howler": "^2.1.2",
    "netjet": "^1.3.0",
    "node-fetch": "^2.4.1",
    "nodemon": "^1.18.10"
  }
}
